\n            .then(json=>{\r\n              setProducts(json)\r\n              localStorage.setItem('defaultProducts', JSON.stringify(json))\r\n              setPrelaod(false)\r\n            })\r\n  },[])\r\n\r\n  \r\n\r\n\r\n  function heandlerNextPrev(direction){\r\n    const ulWidth = ul.current.offsetWidth;\r\n    const li = [...ul.current.getElementsByTagName('li')];\r\n    const liFirst = li[0].offsetWidth;\r\n    const liFirstitem = li[0];\r\n    const liFirst_mr = parseInt(window.getComputedStyle(li[0]).getPropertyValue('margin-right'))\r\n    const liFirstFull_width = liFirst_mr + liFirst\r\n\r\n    let liFirst_ml = Math.abs(parseInt(window.getComputedStyle(li[0]).getPropertyValue('margin-left')))\r\n  \r\n    let ml = 70;\r\n    let breakPoint = li.length*liFirstFull_width - ulWidth\r\n  \r\n    if(liFirst_ml < breakPoint - liFirstFull_width ){ml = liFirst_ml + liFirstFull_width*(direction == 'prev'? -1 : 1);\r\n    } \r\n    \r\n    liFirstitem.style.marginLeft = `-${ml}px` ;\r\n\r\n  }\r\n\r\n  function handleAddProduct(event){\r\n    const id = +event.target.dataset.id\r\n      products.forEach((product)=>{\r\n        let flag = true; \r\n        if(product.id === id){  \r\n                   \r\n           if(basketProduct?.length == 0){\r\n             product.count = 1;\r\n             basketProduct.push(product)\r\n           }else{\r\n            basketProduct.forEach((basketItem)=>{\r\n                  if(basketItem.id == id){                      \r\n                        basketItem.count +=1;\r\n                        flag = false\r\n                        }})\r\n\r\n              if(flag){\r\n                product.count = 1;\r\n                console.log(product)\r\n                basketProduct.push(product)              \r\n                \r\n              }\r\n           \r\n           }\r\n         }\r\n      })\r\n\r\n      localStorage.setItem('basket',JSON.stringify(basketProduct))\r\n      \r\n     setBasketProduct([...basketProduct])\r\n   \r\n\r\n     \r\n     \r\n\r\n  }\r\n  \r\n  return(\r\n      <Fragment>\r\n      \r\n            <div className=\"main_header\">\r\n            \r\n            <div className=\"main_filter\">\r\n              <div className=\"main_filter_name\">Наш каталог</div>\r\n              <button className=\"main_filter_btn\" onClick={()=>{setFilter(true)}}><span className=\"main_filter_logo\"></span></button>\r\n            {filter == true? <FilterModul setFilter={setFilter}/> :''}\r\n            </div>\r\n           \r\n            \r\n            <Search />\r\n          </div>\r\n          <div className=\"catalog\">\r\n            {prelaod ?<Preload/>:<ul ref={ul} className=\"catalog_items\">\r\n              <Context.Provider value={{products, handleAddProduct, setProducts}}>\r\n                  <Products />\r\n              </Context.Provider>\r\n            </ul> }            \r\n            \r\n          </div>\r\n          <div className=\"main_nav\">\r\n              <button className=\"btn_prev\" onClick={()=>{heandlerNextPrev('prev')}} ><span className=\"btn_arrow\">&#8592;</span></button> \r\n              <button className=\"btn_next\" onClick={()=>{heandlerNextPrev('next')}}><span className=\"btn_arrow\">&#8594;</span></button>             \r\n            </div>\r\n    </Fragment>\r\n    )\r\n}\r\n\r\nexport default Catalog","D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\Context.js",[],"D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\FilterModul.js",["123","124","125"],"import React, { useContext } from 'react'\r\nimport UnionContext from './UnionContext'\r\n\r\n\r\nfunction FilterModule({setFilter}){\r\n    const {radio, setRadio} =useContext (UnionContext)\r\n\r\n    return(\r\n        <div className=\"filter_module\" onClick={()=>{setFilter(false)}}>\r\n            <div className=\"filter_body\" onClick={(event)=>{event.stopPropagation()}}>\r\n                <h3>Сортировка по</h3>\r\n                <label><input type=\"radio\" defaultChecked={radio == 'default' ?true:false} name=\"filter\" value=\"default\" onClick={()=>{setRadio('default')}}/>По умолчанию</label>\r\n                <label><input type=\"radio\" defaultChecked={radio == 'abs' ?true:false}  name=\"filter\" value=\"abs\" onClick={()=>{setRadio('abs')}}/>По возрастанию</label>\r\n                <label><input type=\"radio\" defaultChecked={radio == 'desc' ?true:false}  name=\"filter\" value=\"desc\" onClick={()=>{setRadio('desc')}}/>По убыванию</label>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default FilterModule","D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\Products.js",["126","127","128","129","130","131"],"import React, { Fragment, useContext, useEffect } from 'react'\r\nimport Context from './Context'\r\nimport Product from '../Product'\r\nimport UnionContext from './UnionContext'\r\n\r\nfunction Products(){\r\n     const {products, handleAddProduct, setProducts} = useContext(Context)\r\n     const {search, radio} = useContext(UnionContext)\r\n     const defaultProducts = JSON.parse(localStorage.getItem('defaultProducts'))\r\n\r\n     useEffect(()=>{\r\n       if(radio ==\"abs\"){\r\n         products.sort((a,b)=>{\r\n           return a.price - b.price\r\n         })\r\n         setProducts([...products])\r\n       }else if(radio ==\"desc\"){\r\n        products.sort((a,b)=>{\r\n          return b.price - a.price \r\n        })\r\n        setProducts([...products])\r\n       }else if(radio == 'default'){\r\n        setProducts([...defaultProducts])\r\n       }\r\n     },[radio])\r\n\r\n\r\n\r\n\r\n    return(\r\n        <Fragment>\r\n        \r\n        {products.filter((item)=>{\r\n          if(search == ''){\r\n            return item\r\n          }else if(item.title.toLowerCase().includes(search.toLowerCase())){\r\n            return item\r\n          }\r\n          else if(item.collection.toLowerCase().includes(search.toLowerCase())){\r\n            return item\r\n          }\r\n        }).map((product, index)=>{\r\n        return(\r\n         <li className=\"catalog_item\" key={index+new Date().getMilliseconds()}>\r\n             <Product \r\n                id={product.id}\r\n                title ={product.title}\r\n                image ={product.image}\r\n                collection = {product.collection}\r\n                description ={product.description}\r\n                price ={product.price}\r\n                handleAddProduct={handleAddProduct}            \r\n             \r\n             />\r\n        </li> \r\n        )\r\n    })}\r\n        \r\n      </Fragment>    \r\n    )\r\n}\r\n\r\nexport default Products","D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\Preload.js",[],"D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Product.js",["132"],"import React from 'react'\r\nimport { useHistory } from 'react-router'\r\n\r\nfunction Product ({id,title,image,description,price,handleAddProduct,collection}){\r\n    const history = useHistory()\r\n\r\n    function handleClick(){\r\n        history.push(`/product/${id}`,{\r\n            id,\r\n            title,\r\n            description,\r\n            price,\r\n            image,\r\n            collection\r\n        })\r\n\r\n    }\r\n    \r\n    return(\r\n        <>\r\n            <div className=\"catalog_item_img\">\r\n                 <img src={image}/>\r\n            </div>\r\n            <div className=\"catalog_caption\">\r\n            <div className=\"catalog_caption_name\" onClick={()=>{handleClick()}}>{title}</div>\r\n            <div className=\"catalog_caption_collection\">Коллекция {collection}</div>\r\n           \r\n            <div className=\"catalog_caption_price\">{price} $</div>\r\n            <button className=\"catalog_caption_btn\" data-id={id} onClick={(event)=>{handleAddProduct(event)}}>+</button>\r\n            </div>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Product","D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\TodosPage.js",["133"],"D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\TodoList.js",[],"D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\TodoForm.js",[],"D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\About.js",["134","135","136","137"],"import React from 'react'\r\n\r\n\r\n function About (){\r\n   return(\r\n     <div className=\"about_wrapper\">\r\n       <div className=\"about_image\"></div>\r\n       \r\n       <div className=\"about_text_wrapper\">\r\n         Мы проделали огромную работу для того, чтобы подарить каждой невесте её особенное свадебное платье! На сегодняшний день свадебный салон VINTAGE™ в Минске знает каждая невеста! За 10 лет работы этот бренд стал узнаваемым не только в Беларуси, но и за рубежом, что подтверждается многочисленными дипломами.\r\n       </div>\r\n       <div className='diplom_wrapper'>\r\n          <p><img src='https://vintage.by/_thumbs/galleries-gallery/src-2403.webp'/></p>\r\n          <p><img src=' https://vintage.by/_thumbs/galleries-gallery/src-2413.webp'/></p>\r\n          <p><img  src=' https://vintage.by/_thumbs/galleries-gallery/src-2404.webp'/></p>\r\n          <p>< img  src=' https://vintage.by/_thumbs/galleries-gallery/src-2398.webpimg '/></p>\r\n        </div>\r\n        <iframe  className='about_video' src=\"https://www.youtube.com/embed/XHE1XrdqWy0\" title=\"YouTube video player\" ></iframe>\r\n        <div className=\"about_text\">\r\n            <p> -Анна, расскажите, с чего начался VINTAGE.</p>\r\n            <p>-На самом деле история простая и банальная, но в тоже время все закономерно и логично. Все началось больше 10 лет назад... Открыли свадебный салон VINTAGE™ мы с мамой. Чем бы в жизни мы не занимались, где и кем бы не работали, в свою трудовую деятельность постоянно вносили элементы креатива, творчества и созидания.<br />\r\n            Мы учились, познавали, преодолевали. На нашем пути было немало трудностей. Наш секрет в том, что мы принадлежим к тем счастливым людям, для которых хобби и любимое занятие - основная трудовая деятельность. Когда работаешь с полной отдачей и самозабвением, успех непременно будет! То, что мы делаем и то, какого уровня мастерства и профессионализма мы достигли - говорит об этом! О нас знают, нам доверяют, к нам обращаются за советами и услугами. <br />\r\n            Значимое достижение на сегодняшний день - наш коллектив! Наши сотрудники - специалисты высокого уровня с хорошей профессиональной подготовкой в индустрии свадебной моды, а также практической работы с людьми, способные понять, почувствовать, уловить желание и стиль каждой невесты. Именно в этом и заключается индивидуальный подход к каждому клиенту.\"\r\n            </p>\r\n            <p>-А откуда появилось такое название? Почему именно \"Vintage\"? </p>\r\n            <p>-Это емкое по смыслу и значению слово, подразумевает высокое качество, идеальное исполнение, оригинальность, эксклюзивность. Тонкая грань классики и новаторства, гламурный дизайн. Как тут не вспомнить знаменитое выражение: \"Как корабль назовешь, так он и поплывет...\"</p>\r\n         </div>\r\n        \r\n       \r\n     </div>\r\n   )\r\n }\r\n  export default About","D:\\IT-schooll\\JS\\FinalProdject\\wedding-shop\\src\\Componets\\Contact.js",["138"],"import React from 'react'\r\n\r\nfunction Contact (){\r\n  return(\r\n    <div className ='contact_wrapper'>\r\n    <div className ='contact_information'>\r\n      <h1>Мы в самом сердце Минска</h1>\r\n      <p>❤️ ул. Карла Либкнехта, 68</p>\r\n      <h2>ВРЕМЯ РАБОТЫ</h2>\r\n      <p> Понедельник — Cуббота<br/>\r\n          11:00 — 20:00<br/>\r\n          Воскресенье <br/>\r\n          11:00 — 18:00</p>\r\n      <h2>ВСЕГДА НА СВЯЗИ</h2>\r\n      <p>+375 (17) 88-88-800</p>\r\n      <p>vintage@gmail.com</p>\r\n    </div>\r\n    <div className ='contact_map'>\r\n    <iframe   className = 'map' src=\"https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2350.9503372